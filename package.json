    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "release-it": "16.2.1",
    "terser": "5.32.0",
    "tsup": "7.2.0",
    "typescript": "5.5.4",
    "webpack": "5.89.0",
    "webpack-bundle-analyzer": "4.9.1",
