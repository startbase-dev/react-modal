{
  "name": "@start-base/react-modal",
  "author": "Start-Base",
  "version": "1.3.0",
  "description": "Accessible modal dialog component and global modals provider for React.JS. This package is a enhanced version of react-modal.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startbase-dev/react-modal.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "modal",
    "react-modal",
    "modal-context",
    "modal-provider",
    "useModal",
    "global-modals",
    "dialog",
    "react",
    "react-component",
    "start-base",
    "ui"
  ],
  "main": "dist/lib/index.cjs",
  "module": "dist/lib/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js --fix",
    "format": "prettier --write .",
    "stylelint": "stylelint \"**/*.css\" --fix",
    "unimported": "npx unimported",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:lib": "tsup src/index.js --out-dir dist/lib --no-splitting --treeshake --minify terser"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-modal": "^3.0.0"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-modal": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.4",
    "@babel/preset-env": "7.24.4",
    "@babel/preset-react": "7.24.1",
    "autoprefixer": "10.4.19",
    "babel-loader": "9.1.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-compat": "4.2.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "tsup": "8.0.2",
    "webpack": "5.91.0",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4"
  },
  "tsup": {
    "format": [
      "esm",
      "cjs"
    ],
    "loader": {
      ".js": "jsx"
    },
    "dts": false,
    "sourcemap": false,
    "clean": true
  },
  "bugs": {
    "url": "https://github.com/startbase-dev/react-modal/issues"
  },
  "homepage": "https://github.com/startbase-dev/react-modal#readme"
}